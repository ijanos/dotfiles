Config { font = "-*-fixed-*-*-*-*-10-*-*-*-*-*-*-u"
       , bgColor = "black"
       , fgColor = "grey"
       , position = Top
       , lowerOnStart = True
       , commands = [ Run StdinReader
                    , Run MultiCpu ["-t", "<autototal>"
                                   ,"-L","9"
                                   ,"-H","60","--normal","green"
                                   ,"--high","red"]
                      9
                    , Run Memory ["-t","<used>MiB"] 10
                    , Run Date "<fc=#999999>%Y-%m-%_d</fc> <fc=#FFFFFF>%H:%M</fc>" "date" 9
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %multicpu% | %memory% | %date%"
       }

